		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>2</partEfficiency>
		</addedPartProps>

		<comps>
			<li Class="HE_AntiReality.HediffCompProperties_NeedHediff">
				<needHediffs>
					<li>
						<hediffDefs>
							<li>AR_MultiImplantor</li>
						</hediffDefs>
					</li>

				</needHediffs>
				<mode>Multi</mode>
				<!--必要ない時はNone-->
			</li>
		</comps>
	</HediffDef>

