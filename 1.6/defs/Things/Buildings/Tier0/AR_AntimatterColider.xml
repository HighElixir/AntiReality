		<size>(1,1)</size>
		<costList>
			<Plasteel>150</Plasteel>
			<Uranium>220</Uranium>
			<ComponentSpacer>5</ComponentSpacer>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceTrader">
				<pipeNet>AR_FictionPipeSystem</pipeNet>
				<consumptionPerTick>-0.005</consumptionPerTick>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>18500</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>Things/Special/Fluid/AR_Pipe_Overlay</commandTexture>
				<commandLabelKey>AR_CommandToggleFE_Label</commandLabelKey>
				<commandDescKey>AR_CommandToggleFE_Desc</commandDescKey>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>6</glowRadius>
				<glowColor>(90, 110, 110, 140)</glowColor>
			</li>
			<li Class="AntiReality.CompProperties_SpawnerItemsWithPower">
				<spawnThings>
					<li>Tear_in_Reality</li>
				</spawnThings>
			</li>
			<!--<li Class="CompProperties_Breakdownable" />-->
			<!--<li Class="CompProperties_Explosive">
				<explosiveRadius>0</explosiveRadius>
				<explosiveExpandPerFuel>0.95</explosiveExpandPerFuel>
				<explosiveDamageType>Bathed_in_Energy</explosiveDamageType>
				<destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<wickTicks>70~150</wickTicks>
			</li>-->
		</comps>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationHotKey>Misc4</designationHotKey>
		<constructEffect>ConstructMetal</constructEffect>
		<researchPrerequisites>
			<li>Research_AR_AntimatterColider</li>
		</researchPrerequisites>
		<designationCategory>AR_AntiRealConstructs</designationCategory>
		<constructionSkillPrerequisite>12</constructionSkillPrerequisite>
