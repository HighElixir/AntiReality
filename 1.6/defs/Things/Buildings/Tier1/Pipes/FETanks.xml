<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>AR_FETank</defName>
		<label>小虚構発生装置域</label>
		<description>小規模な異界を作成し、その中に流体虚構エネルギーを貯蔵する。</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/AR_FETank</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.40</fillPercent>
		<tickerType>Normal</tickerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>AR_Powers</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>12000</MaxHitPoints>
			<WorkToBuild>27000</WorkToBuild>
			<Mass>20</Mass>
			<Flammability>0</Flammability>
			<Beauty>3.5</Beauty>
		</statBases>
		<size>(2,2)</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Medium</destroySound>
		</building>
		<costList>
			<Displacement_Plasteel>45</Displacement_Plasteel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceStorage">
				<!-- Accept a PipeNetDef -->
				<pipeNet>AR_FictionPipeSystem</pipeNet>
				<!-- Amount that can be stored -->
				<storageCapacity>500</storageCapacity>
				<!-- Lenght and Width of the bar -->
				<barSize>(1.3, 0.25)</barSize>
				<!-- 0 mean there is no bar outline -->
				<margin>0.1</margin>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>6</glowRadius>
				<glowColor>(90, 110, 110, 140)</glowColor>
			</li>
		</comps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<uiOrder>2020</uiOrder>
		<staticSunShadowHeight>0.2</staticSunShadowHeight>
		<constructEffect>ConstructMetal</constructEffect>
		<designationHotKey>Misc4</designationHotKey>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>AR_Research_FETank</li>
		</researchPrerequisites>
		<designationCategory>AR_AntiRealConstructs</designationCategory>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
	</ThingDef>
</Defs>